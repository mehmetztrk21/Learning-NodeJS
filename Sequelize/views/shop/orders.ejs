<%- include('../includes/head.ejs') %>
</head>

<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <% if(orders.length<=0) { %>
        <h1>Nothing there!</h1>

        <% } else { %>
        <ul>
            <% for(let order of orders){ %>
            <li><h1>#<%=order.id%></h1></li>
            <ul>
                <%for(let item of order.products){%>
                <li><%=item.title%> (<%=item.orderItem.quantity%>)</li>
                <%}%>
                    </ul>
                    <%}%>
            </ul>
            <%}%>
        </main>
        <%- include('../includes/end.ejs') %>